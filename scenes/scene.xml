	<!-- Grass -->
    <TEXTURE id="grass">
        <file path="images/grass.jpg" />                
        <amplif_factor s="4" t="5" />                           
    </TEXTURE>
	
	<!-- Road -->
    <TEXTURE id="road">
        <file path="images/road.jpg" />                
